          <% else %>
            <div class="hidden-xs">
                  <%= form_tag({controller: "requests", action: "create"}, method:'post') do %>
                  <input name="request[phoneNumber]" id="phone_number" placeholder="Your Email" class="input-lg" style="text-align:center;color:black">
                  <br><br>
                  <input type="submit" name="commit" id="submitphone" value="See Your Credit Card's Ranking In 15 Seconds" class="btn btn-warning btn-lg">
                  <% end %>
            </div>
          <% end %>



            <div class="visible-xs-block">
              <a href="sms:6692365527&body=Hi, Benjamin" class="btn btn-warning btn-lg">Start With A Text</a>
            </div>
          </div>
        </div>
      </div>
    </div>
